#: better-font-awesome.php:668
msgid ""
"<h3>Usage</h3>\n"
"\t\t\t\t\t <b>Font Awesome version 4.x +</b>&nbsp;&nbsp;&nbsp;<small><a href=\"http://fontawesome.io/examples/\">See all available options &raquo;</a></small><br /><br />\n"
"\t\t\t\t\t <i class=\"icon-coffee fa fa-coffee\"></i> <code>[icon name=\"coffee\"]</code> or <code>&lt;i class=\"fa-coffee\"&gt;&lt;/i&gt;</code><br /><br />\n"
"\t\t\t\t\t <i class=\"icon-coffee fa fa-coffee icon-2x fa-2x\"></i> <code>[icon name=\"coffee\" class=\"fa-2x\"]</code> or <code>&lt;i class=\"fa-coffee fa-2x\"&gt;&lt;/i&gt;</code><br /><br />\n"
"\t\t\t\t\t <i class=\"icon-coffee fa fa-coffee icon-2x fa-2x icon-rotate-90 fa-rotate-90\"></i> <code>[icon name=\"coffee\" class=\"fa-2x fa-rotate-90\"]</code> or <code>&lt;i class=\"fa-coffee fa-2x fa-rotate-90\"&gt;&lt;/i&gt;</code><br /><br /><br />\n"
"\t\t\t\t\t <b>Font Awesome version 3.x</b>&nbsp;&nbsp;&nbsp;<small><a href=\"http://fontawesome.io/3.2.1/examples/\">See all available options &raquo;</a></small><br /><br />\n"
"\t\t\t\t\t <i class=\"icon-coffee fa fa-coffee\"></i> <code>[icon name=\"coffee\"]</code> or <code>&lt;i class=\"icon icon-coffee\"&gt;&lt;/i&gt;</code><br /><br />\n"
"\t\t\t\t\t <i class=\"icon-coffee fa fa-coffee icon-2x fa-2x\"></i> <code>[icon name=\"coffee\" class=\"icon-2x\"]</code> or <code>&lt;i class=\"icon icon-coffee icon-2x\"&gt;&lt;/i&gt;</code><br /><br />\n"
"\t\t\t\t\t <i class=\"icon-coffee fa fa-coffee icon-2x fa-2x icon-rotate-90 fa-rotate-90\"></i> <code>[icon name=\"coffee\" class=\"icon-2x icon-rotate-90\"]</code> or <code>&lt;i class=\"icon icon-coffee icon-2x icon-rotate-90\"&gt;&lt;/i&gt;</code>"
msgstr ""
"<h3>Uso</h3>\n"
"\t\t\t\t\t <b>Font Awesome versión 4.x +</b>&nbsp;&nbsp;&nbsp;<small><a href=\"http://fontawesome.io/examples/\">Ver todas las opciones disponibles &raquo;</a></small><br /><br />\n"
"\t\t\t\t\t <i class=\"icon-coffee fa fa-coffee\"></i> <code>[icon name=\"coffee\"]</code> o <code>&lt;i class=\"fa-coffee\"&gt;&lt;/i&gt;</code><br /><br />\n"
"\t\t\t\t\t <i class=\"icon-coffee fa fa-coffee icon-2x fa-2x\"></i> <code>[icon name=\"coffee\" class=\"fa-2x\"]</code> o <code>&lt;i class=\"fa-coffee fa-2x\"&gt;&lt;/i&gt;</code><br /><br />\n"
"\t\t\t\t\t <i class=\"icon-coffee fa fa-coffee icon-2x fa-2x icon-rotate-90 fa-rotate-90\"></i> <code>[icon name=\"coffee\" class=\"fa-2x fa-rotate-90\"]</code> o <code>&lt;i class=\"fa-coffee fa-2x fa-rotate-90\"&gt;&lt;/i&gt;</code><br /><br /><br />\n"
"\t\t\t\t\t <b>Font Awesome versión 3.x</b>&nbsp;&nbsp;&nbsp;<small><a href=\"http://fontawesome.io/3.2.1/examples/\">Ver todas las opciones disponibles &raquo;</a></small><br /><br />\n"
"\t\t\t\t\t <i class=\"icon-coffee fa fa-coffee\"></i> <code>[icon name=\"coffee\"]</code> o <code>&lt;i class=\"icon icon-coffee\"&gt;&lt;/i&gt;</code><br /><br />\n"
"\t\t\t\t\t <i class=\"icon-coffee fa fa-coffee icon-2x fa-2x\"></i> <code>[icon name=\"coffee\" class=\"icon-2x\"]</code> o <code>&lt;i class=\"icon icon-coffee icon-2x\"&gt;&lt;/i&gt;</code><br /><br />\n"
